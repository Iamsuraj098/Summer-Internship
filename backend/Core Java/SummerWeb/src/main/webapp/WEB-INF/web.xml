<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>Example</display-name>
	<servlet>
		<servlet-name>Config-example</servlet-name>
		<jsp-file>/Display_config.jsp</jsp-file>
		<init-param>
			<param-name>fname</param-name>
			<param-value>Ratnesh</param-value>
		</init-param>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Config-example</servlet-name>
		<url-pattern>/Display_config</url-pattern>
	</servlet-mapping>
	
	<!-- Application implicit object-->
	<context-param>
		<param-name>Branch</param-name>
		<param-value>CSE</param-value>
	</context-param>
	
</web-app>





